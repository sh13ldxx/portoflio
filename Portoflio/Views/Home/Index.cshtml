        </div>
      </div>
    </div>
  </section>

  <!-- End Skills Section -->






  <!-- Start Contact Section -->

  <section id="contacts" class="contact">
    <div class="container">

      <div class="section-title">
        <h2>Contact</h2>
      </div>

      <div class="row mt-2">
          @foreach (var item in Model.Abouts)
         {
             
        <div class="col-md-6 d-flex align-items-stretch">
          <div class="info-box">
            <i class="bx bx-map"></i>
            <h3>My Address</h3>

            <p>@item.City</p>
          </div>
        </div>
         }

        <div class="col-md-6 mt-4 mt-md-0 d-flex align-items-stretch">
          <div class="info-box">
            <i class="bx bx-share-alt"></i>
            <h3>Social Profiles</h3>
            <div class="social-links">
                          @foreach (var item in Model.Socials)
         {
              <a href="@item.Value" target="_blank" class="@item.Key"><i class="bx bxl-@item.Key"></i></a>
             
         }

            </div>
          </div>
        </div>
        @foreach (var item in Model.Abouts)
       {
           
        <div class="col-md-6 mt-4 d-flex align-items-stretch">
          <div class="info-box">
            <i class="bx bx-envelope"></i>
            <h3>Email</h3>
            <p>@item.Email</p>
          </div>
        </div>
        <div class="col-md-6 mt-4 d-flex align-items-stretch">
          <div class="info-box">
            <i class="bx bx-phone-call"></i>
            <h3>Contact</h3>
            <p>@item.Phone</p>
          </div>
        </div>
       }
      </div>
